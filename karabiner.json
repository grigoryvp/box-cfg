{
"global": {
  "check_for_updates_on_startup": true,
  "show_in_menu_bar": true,
  "show_profile_name_in_menu_bar": false
},
"profiles": [{
  "devices": [{
    "disable_built_in_keyboard_if_exists": false,
    "fn_function_keys": [],
    "identifiers": {
      "is_keyboard": true,
      "is_pointing_device": false,
      "product_id": 636,
      "vendor_id": 1452
    },
    "ignore": false,
    "manipulate_caps_lock_led": true,
    "simple_modifications": []
  }],
  "fn_function_keys": [],
  "name": "Default profile",
  "selected": true,
  "simple_modifications": [{
    "from": {"key_code": "tab"},
    "to": {"key_code": "left_control"}
  }, {
    "from": {"key_code": "return_or_enter"},
    "to": {"key_code": "right_control"}
  }],
  "virtual_hid_keyboard": {
      "country_code": 0
  },
  "complex_modifications": {
    "parameters": {
      "basic.simultaneous_threshold_milliseconds": 50,
      "basic.to_delayed_action_delay_milliseconds": 500,
      "basic.to_if_alone_timeout_milliseconds": 1000,
      "basic.to_if_held_down_threshold_milliseconds": 500
    },
    "rules": [{
      "description": "grigoryvp keyboard mods",
      "manipulators": [
      
      {
        "type": "basic",
        "from": {"key_code": "caps_lock"},
        "to": [{
          "set_variable": {
            "name": "_meta",
            "value": 1
          }
        }],
        "to_after_key_up": [{
          "set_variable": {
            "name": "_meta",
            "value": 0
          }
        }]
      },

      {
        "type": "basic",
        "from": {
          "key_code": "left_control",
          "modifiers": {"optional": ["any"]}
        },
        "to": [{
          "key_code": "left_control",
          "lazy": true
        }],
        "to_if_alone": [{
          "key_code": "tab"
        }]
      },

      {
        "type": "basic",
        "from": {
          "key_code": "right_control",
          "modifiers": {"optional": ["any"]}
        },
        "to": [{
          "key_code": "right_control",
          "lazy": true
        }],
        "to_if_alone": [{
          "key_code": "return_or_enter"
        }]
      },

      {
        "type": "basic",
        "conditions": [{"type": "variable_if", "name": "_meta", "value": 1}],
        "from": {
          "key_code": "open_bracket",
          "modifiers": {"optional": ["any"]}
        },
        "to": [{"key_code": "escape"}]
      },

      {
        "type": "basic",
        "conditions": [{"type": "variable_if", "name": "_meta", "value": 1}],
        "from": {
          "key_code": "p",
          "modifiers": {"optional": ["any"]}
        },
        "to": [{"key_code": "delete_or_backspace"}]
      },

      {
        "type": "basic",
        "conditions": [{"type": "variable_if", "name": "_meta", "value": 1}],
        "from": {
          "key_code": "semicolon",
          "modifiers": {"optional": ["any"]}
        },
        "to": [{"pointing_button": "button1"}]
      },

      {
        "type": "basic",
        "conditions": [{"type": "variable_if", "name": "_meta", "value": 1}],
        "from": {
          "key_code": "quote",
          "modifiers": {"optional": ["any"]}
        },
        "to": [{"pointing_button": "button2"}]
      },

      {
        "type": "basic",
        "conditions": [{"type": "variable_if", "name": "_meta", "value": 1}],
        "from": {
          "key_code": "h",
          "modifiers": {"optional": ["any"]}
        },
        "to": [{"key_code": "left_arrow"}]
      },

      {
        "type": "basic",
        "conditions": [{"type": "variable_if", "name": "_meta", "value": 1}],
        "from": {
          "key_code": "j",
          "modifiers": {"optional": ["any"]}
        },
        "to": [{"key_code": "down_arrow"}]
      },

      {
        "type": "basic",
        "conditions": [{"type": "variable_if", "name": "_meta", "value": 1}],
        "from": {
          "key_code": "k",
          "modifiers": {"optional": ["any"]}
        },
        "to": [{"key_code": "up_arrow"}]
      },

      {
        "type": "basic",
        "conditions": [{"type": "variable_if", "name": "_meta", "value": 1}],
        "from": {
          "key_code": "l",
          "modifiers": {"optional": ["any"]}
        },
        "to": [{"key_code": "right_arrow"}]
      },

      {
        "type": "basic",
        "conditions": [{"type": "variable_if", "name": "_meta", "value": 1}],
        "from": {
          "key_code": "7"
        },
        "to": [{
          "shell_command": "open '/Applications/Utilities/Terminal.app'"
        }]
      },

      {
        "type": "basic",
        "conditions": [{"type": "variable_if", "name": "_meta", "value": 1}],
        "from": {
          "key_code": "8"
        },
        "to": [{
          "shell_command": "open '/Applications/Visual Studio Code.app'"
        }]
      },

      {
        "type": "basic",
        "conditions": [{"type": "variable_if", "name": "_meta", "value": 1}],
        "from": {
          "key_code": "9"
        },
        "to": [{
          "shell_command": "open '/Applications/Safari.app'"
        }]
      },

      {
        "type": "basic",
        "conditions": [{"type": "variable_if", "name": "_meta", "value": 1}],
        "from": {
          "key_code": "0"
        },
        "to": [{
          "shell_command": "open '/Applications/Rambox.app'"
        }]
      },

      {
        "type": "basic",
        "conditions": [{"type": "variable_if", "name": "_meta", "value": 1}],
        "from": {
          "key_code": "backslash"
        },
        "to": [{
          "shell_command": "open '/Applications/Launchpad.app'"
        }]
      }]
    }]
  }
}]
}
