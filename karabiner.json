            "key_code": "right_control",
            "lazy": true
          }],
          "to_if_alone": [{
            "key_code": "return_or_enter"
          }]
        },
  
        {
          "type": "basic",
          "conditions": [{"type": "variable_if", "name": "_m", "value": 1}],
          "from": {
            "key_code": "open_bracket",
            "modifiers": {"optional": ["any"]}
          },
          "to": [{"key_code": "escape"}]
        },
  
        {
          "type": "basic",
          "conditions": [{"type": "variable_if", "name": "_m", "value": 1}],
          "from": {
            "key_code": "close_bracket",
            "modifiers": {"optional": ["any"]}
          },
          "to": [{"key_code": "tab", "modifiers": ["left_command"]}]
